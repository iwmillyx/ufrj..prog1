#include <stdio.h>

int main(){
    
    int cent, unid, dezen, N;
     
     printf("Digite um número com 3 dígitos: ");
     scanf("%d", &N);
     
    if(N > 999 || N < 99){
        printf("VAI FAZER DE NOVO SUA VAGABUNDA \n");
    }
    
    else{
        cent= N/100;
        dezen= (N%100)/10;
        unid= N%10;
    }
     printf("O resultado é: %d %d %d", cent, dezen, unid);
     
    return 0;
}
